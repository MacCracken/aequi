{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Aequi",
  "identifier": "com.aequi.app",
  "version": "0.2.0",
  "build": {
    "beforeDevCommand": "",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "",
    "frontendDist": "../../src"
  },
  "app": {
    "windows": [
      {
        "title": "Aequi - Open Source Accounting",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "iOS": {
      "minimumSystemVersion": "13.0"
    },
    "android": {
      "minSdkVersion": 24
    }
  }
}
